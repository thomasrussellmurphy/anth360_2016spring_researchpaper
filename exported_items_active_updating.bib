  pages = {88--92},
  keywords = {Abortion training,medical education,Opt-out,Resident education},
  file = {ScienceDirect Full Text PDF:/Users/thomasrussellmurphy/Library/Application Support/Zotero/Profiles/8m1mieef.default/zotero/storage/BFMN7ZD7/Steinauer et al. - 2013 - Opting out of abortion training benefits of parti.pdf:application/pdf;ScienceDirect Snapshot:/Users/thomasrussellmurphy/Library/Application Support/Zotero/Profiles/8m1mieef.default/zotero/storage/U7HATN2R/S0010782412008128.html:}
