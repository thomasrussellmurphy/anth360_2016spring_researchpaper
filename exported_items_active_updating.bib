  date = {2010-06},
  pages = {525--530},
  keywords = {ABORTION,Opt-out,Professionalism,Qualitative,Residency training,Values clarification},
  file = {ScienceDirect Full Text PDF:/Users/thomasrussellmurphy/Library/Application Support/Zotero/Profiles/8m1mieef.default/zotero/storage/5UGUEBPE/Freedman et al. - 2010 - Obstetrician-gynecologist experiences with abortio.pdf:application/pdf;ScienceDirect Snapshot:/Users/thomasrussellmurphy/Library/Application Support/Zotero/Profiles/8m1mieef.default/zotero/storage/7QZU3JUM/S0010782410000041.html:}
