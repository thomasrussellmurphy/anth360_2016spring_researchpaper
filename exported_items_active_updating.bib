  number = {4},
  journaltitle = {Contraception},
  shortjournal = {Contraception},
  author = {Turk, Jema K. and Preskill, Felisa and Landy, Uta and Rocca, Corinne H. and Steinauer, Jody E.},
  urldate = {2016-03-15},
  date = {2014-04},
  pages = {271--277},
  keywords = {Access,Procedural experience,Resident education},
  file = {ScienceDirect Full Text PDF:/Users/thomasrussellmurphy/Library/Application Support/Zotero/Profiles/8m1mieef.default/zotero/storage/ENWTQQJA/Turk et al. - 2014 - Availability and characteristics of abortion train.pdf:application/pdf;ScienceDirect Snapshot:/Users/thomasrussellmurphy/Library/Application Support/Zotero/Profiles/8m1mieef.default/zotero/storage/UA4H5ZHS/S0010782413007452.html:}
