@thesis{freedman_willing_2008,
  location = {{United States -- California}},
  title = {Willing and unable: {{Doctors}}' constraints in abortion care},
  rights = {Copyright ProQuest, UMI Dissertations Publishing 2008},
  url = {http://search.proquest.com/docview/304645872/abstract/A420EFAD0541B5PQ/1},
  shorttitle = {Willing and unable},
  abstract = {This dissertation investigates how the politics of abortion have impacted its medical practice in the United States. While residency abortion training has increased nationally since its post-legalization low in the early 1990s, the number of abortion providers continues to decrease. Mainstream physicians rarely integrate abortion into their medical practices. Instead, the responsibility for providing abortion has been consolidated within marginalized abortion clinics beleaguered by anti-abortion harassment and legislation.
Forty in-depth interviews, most with physicians of obstetrics and gynecology throughout the United States, were conducted in order to understand why physicians typically cease to provide abortion after residency. While abortion stigma and contention provide some obstacles, the dissertation shows that willing physicians are further encumbered by changes in the profession of medicine and organization of health care since the 1980s which have shifted the locus of power away from individual physicians and into the hands of their managers, employers, and insurers who may have different preferences around abortion practice.
Data from physician interviews are used to show how barriers such as workplace prohibitions, harassment, and fear of community boycott mediate doctors' sense of professional obligation toward abortion care. These concerns are often bigger deterrents than fear of violence from anti-abortion activists. This study presents a challenge to the commonly held assumptions that physicians decide whether or not to provide abortion based upon personal ideology. Rather, the failure to provide abortion among willing physicians is often structural rather than individual.
Institutional blockades, routine outsourcing, and professional intimidation hinder the integration of abortion services. The growing presence of Catholic-owned health care networks--that under the legal protection of conscience clauses prohibit employees to deliver abortion and contraceptive services--further complicates and constrains physician practices, even in certain cases of miscarriage management. For the most part, only physicians who work in insulated environments such as academic medical settings manage to perform abortions without "subspecializing" (doing almost exclusively abortions). Given the high demand for abortion, the structural barriers to its mainstream practice effectively constitute widespread institutionalized buck-passing, funneling abortion patients to freestanding clinics. Policy implications and suggestions are discussed in the conclusion.},
  pagetotal = {197},
  timestamp = {2016-04-25T07:06:47Z},
  langid = {english},
  institution = {{University of California, Davis}},
  type = {Ph.{{D}}.},
  author = {Freedman, Lori Rachel},
  urldate = {2016-04-25},
  date = {2008},
  keywords = {ABORTION,Doctors,Health care,Marginalization,Medical profession,Physician autonomy,Reproductive health,Social sciences,Stigma},
  file = {Full Text PDF:/Users/thomasrussellmurphy/Library/Application Support/Zotero/Profiles/8m1mieef.default/zotero/storage/QB89BTHI/Freedman - 2008 - Willing and unable Doctors' constraints in aborti.pdf:application/pdf}
}

